      key <AC11> { [      0x1000A1F, 0x1000A20 	]	};
      key <BKSL> { [      backslash, bar	]	};

      key <AB01> { [		  z, Z		]       };
      key <AB02> { [      0x1000A02, 0x1000A70  ]       };
      key <AB03> { [      0x1000A2E, 0x1000A23  ]       };
