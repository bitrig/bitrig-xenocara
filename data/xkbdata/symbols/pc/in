xkb_symbols "guj" {
      name[Group1]= "India - Gujarati";

      // Mainly numbers.
      key <AE01> { [      0x1000AE7, 0x1000A8D 	]	};
      key <AE02> { [      0x1000AE8, 0x1000AC5 	]	};
      key <AE03> { [      0x1000AE9 	 	]	};
      key <AE04> { [      0x1000AEA  	 	]	};
      key <AE05> { [      0x1000AEB  	 	]	};
