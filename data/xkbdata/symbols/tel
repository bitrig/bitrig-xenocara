    key <AD10> { [], [ 	0x01000c1c, 0x01000c1d	] };
    key <AD11> { [], [  0x01000c21, 0x01000c22	] };
    key <AD12> { [], [  0x01000c3c, 0x01000c1e	] };
  
    key <AC01> { [], [  0x01000c4b, 0x01000c13	] };
