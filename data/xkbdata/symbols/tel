    key <AE10> { [], [  0x01000c66, parenright  ] };
    key <AE11> { [], [  0x01000c03, underscore 	] };
    key <AE12> { [], [  0x01000c43, 0x01000c0b  ] };
    key <BKSP> { [], [  BackSpace		] };    
 
    key <AD01> { [], [  0x01000c4c, 0x01000c14	] };
