    echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done

  test -z "$ac_cv_path_krb5_config" && ac_cv_path_krb5_config="not_found"
  ;;
esac
