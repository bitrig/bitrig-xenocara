MISC SUN OPENWINHOME=/usr/openwin.2
MISC SUN LD_LIBRARY_PATH=/usr/openwin.2/lib
MISC SUN FONTPATH=/usr/openwin.2/lib/fonts
MISC SUN HELPPATH=/usr/openwin.2/lib/help
MISC SUN MANPATH=/usr/openwin.2/share/man:/usr/man
# But note that OW2 appears to come with an empty app-defaults!
MISC SUN XAPPLRESDIR=/usr/openwin.2/lib/app-defaults
MISC POSIX PATH=:/usr/openwin.2/bin:/usr/openwin.2/demo:/usr/openwin.2/bin/xview:/usr/ucb:/usr/bin
INTERNAL-AUTH-PROGRAM X11 xauth xauth
INTERNAL-AUTH-INPUT X11 add\040$1\040$2\040$3
