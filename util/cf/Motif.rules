#ifndef VistaMapRule
#define VistaMapRule(targetmap,depend)					@@\
all:: targetmap								@@\
									@@\
targetmap: depend 							@@\
	$(RM) $(MTOP)/tests/Vista/targetmap				@@\
	CATCMD *.M > $(MTOP)/tests/Vista/targetmap
