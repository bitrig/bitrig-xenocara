#ifndef AssembleObject
#define AssembleObject(basename,flags) $(RM) $@                           @@\
	$(CPP) AsmDefines $(DEFINES) $(INCLUDES) flags basename.S | \     @@\
	    grep -v '^\#' > basename.i.s                                  @@\
	$(AS) -o $*.o basename.i.s                                        @@\
	$(RM) basename.i.s
#endif

