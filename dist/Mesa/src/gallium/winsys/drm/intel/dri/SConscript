Import('*')

env = drienv.Clone()

env.ParseConfig('pkg-config --cflags --libs libdrm_intel')

env.Prepend(LIBS = [
    st_dri,
    inteldrm,
    i915,
    trace,
    mesa,
    glsl,
    gallium
])

env.LoadableModule(
    target ='i915_dri.so',
    source = COMMON_GALLIUM_SOURCES,
    SHLIBPREFIX = '',
)
