SHARED_LIBS= Xaw7 12.0 Xaw8 13.0

CONFIGURE_ARGS= --disable-xaw6 --disable-xaw7-symlink --enable-xaw8

afterinstall:
	-rm -f $(DESTDIR)$(LIBDIR)/libXaw.a
	(cd $(DESTDIR)$(LIBDIR) && ln -s libXaw8.a libXaw.a)

.include <bsd.xorg.mk>
